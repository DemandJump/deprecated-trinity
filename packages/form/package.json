{
  "name": "@demandjump/form",
  "version": "1.0.3",
  "description": "Demandjump React Form Components with Formik",
  "main": "index.js",
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --loader \".js=jsx\" --dts",
    "dev": "tsup src/index.ts --format esm,cjs --loader \".js=jsx\" --watch --dts",
    "clean": "rm -rf dist",
    "clean:nm": "rm -rf node_modules",
    "test": "echo \"Error: no test specified\" && exit 1",
    "publish": "yarn npm publish --tolerate-republish --access public",
    "publish:canary": "yarn npm publish --access public --tag canary"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/demandjump/trinity.git"
  },
  "keywords": [
    "trinity",
    "form",
    "formik",
    "demandjump",
    "components",
    "react"
  ],
  "author": "Bryan Overbey",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/demandjump/trinity/issues"
  },
  "homepage": "https://github.com/demandjump/trinity#readme",
  "dependencies": {
    "formik": "^2.2.9",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "tsup": "^5.12.7",
    "typescript": "^4.6.4",
    "yup": "^0.32.11"
  },
  "publishConfig": {
    "access": "public"
  }
}
