{
  "name": "@demandjump/trinity",
  "version": "1.0.5",
  "description": "Public Demandjump React Components and UIKit",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DemandJump/trinity.git"
  },
  "author": "Bryan Overbey <bryanmorganoverbey@gmail.com>",
  "license": "MIT",
  "packageManager": "yarn@3.2.2",
  "bugs": {
    "url": "https://github.com/DemandJump/trinity/issues"
  },
  "homepage": "https://github.com/DemandJump/trinity#readme",
  "scripts": {
    "build": "preconstruct build",
    "fix": "preconstruct fix",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "start-storybook -p 6006",
    "storybook": "start-storybook -p 6006",
    "deploy-storybook": "storybook-to-ghpages",
    "build-storybook": "build-storybook -o docs-build"
  },
  "keywords": [
    "demandjump",
    "uikit",
    "component",
    "react",
    "library"
  ],
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "eslintConfig": {
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "resolutions": {
    "react": "18.2.0"
  },
  "dependencies": {
    "@commitlint/cli": "^16.3.0",
    "@commitlint/config-conventional": "^16.2.4",
    "@storybook/addon-a11y": "^6.5.4",
    "@storybook/addon-essentials": "^6.5.4",
    "@storybook/react": "^6.5.10",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^28.1.6",
    "@types/node": "^14.18.17",
    "@types/react": "18.0.17",
    "@types/react-dom": "^18.0.6",
    "classnames": "^2.3.1",
    "css-loader": "^5.2.7",
    "dependency-check": "^4.1.0",
    "eslint": "^7.32.0",
    "husky": "^6.0.0",
    "is-ci": "^3.0.1",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "jest-environment-jsdom-global": "^3.1.2",
    "lint-staged": "^11.2.6",
    "postcss-modules": "^4.3.1",
    "prettier": "^2.6.2",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.3.0",
    "sass-loader": "^10.2.1",
    "style-loader": "^2.0.0",
    "ts-jest": "^28.0.7"
  },
  "devDependencies": {
    "@mdx-js/react": "^2.1.2",
    "@preconstruct/cli": "^2.2.1",
    "@storybook/addon-actions": "^6.5.9",
    "@storybook/addon-docs": "^6.5.9",
    "@storybook/addon-interactions": "^6.5.9",
    "@storybook/addon-links": "^6.5.9",
    "@storybook/builder-webpack4": "^6.5.9",
    "@storybook/manager-webpack4": "^6.5.9",
    "@storybook/storybook-deployer": "^2.8.12",
    "@storybook/testing-library": "^0.0.13"
  },
  "preconstruct": {
    "packages": [
      "packages/*"
    ]
  }
}
