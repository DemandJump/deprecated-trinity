{
  "name": "@demandjump/trinity",
  "version": "1.1.0",
  "description": "Public Demandjump React Components and UIKit",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DemandJump/trinity.git"
  },
  "author": "Bryan Overbey <bryanmorganoverbey@gmail.com>",
  "license": "MIT",
  "packageManager": "yarn@3.2.2",
  "bugs": {
    "url": "https://github.com/DemandJump/trinity/issues"
  },
  "homepage": "https://github.com/DemandJump/trinity#readme",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "scripts": {
    "test": "jest",
    "build": "rollup -c",
    "start": "start-storybook -p 6006",
    "storybook": "start-storybook -p 6006",
    "deploy-storybook": "storybook-to-ghpages",
    "build-storybook": "build-storybook -o docs-build",
    "svgr": "svgr --icon --title-prop --no-dimensions --typescript -d src/components/Icons assets"
  },
  "keywords": [
    "demandjump",
    "uikit",
    "component",
    "react",
    "library"
  ],
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "eslintConfig": {
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "dependencies": {
    "@commitlint/cli": "^16.3.0",
    "@commitlint/config-conventional": "^16.2.4",
    "@emotion/react": "11.7.1",
    "@emotion/styled": "11.6.0",
    "@fontsource/source-sans-pro": "^4.5.10",
    "@mui/material": "5.4.2",
    "@mui/utils": "5.4.1",
    "classnames": "^2.3.1",
    "dependency-check": "^4.1.0",
    "eslint": "^7.32.0",
    "formik": "^2.2.9",
    "husky": "^6.0.0",
    "is-ci": "^3.0.1",
    "lint-staged": "^11.2.6",
    "postcss-modules": "^4.3.1",
    "prettier": "^2.6.2",
    "prop-types": "^15.8.1",
    "react": "^latest",
    "react-dom": "latest",
    "react-router-dom": "^6.3.0",
    "react-select": "^5.4.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.16.5",
    "@mdx-js/react": "^2.1.2",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.1.1",
    "@rollup/plugin-typescript": "^8.3.0",
    "@storybook/addon-actions": "^6.5.9",
    "@storybook/addon-docs": "^6.5.9",
    "@storybook/addon-essentials": "^6.4.9",
    "@storybook/addon-interactions": "^6.5.9",
    "@storybook/addon-links": "^6.5.9",
    "@storybook/builder-webpack4": "^6.5.9",
    "@storybook/manager-webpack4": "^6.5.9",
    "@storybook/react": "^6.4.9",
    "@storybook/storybook-deployer": "^2.8.12",
    "@storybook/testing-library": "^0.0.13",
    "@svgr/cli": "^6.3.1",
    "@types/node": "^14.18.17",
    "@types/react": "latest",
    "@types/react-dom": "latest",
    "@types/styled-components": "^5.1.19",
    "@types/uuid": "^8.3.4",
    "babel-loader": "^8.2.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rollup": "^2.62.0",
    "rollup-plugin-dts": "^4.1.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-styles": "^4.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "styled-components": "^5.3.3",
    "typescript": "^4.5.4"
  }
}
